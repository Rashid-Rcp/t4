{
  "expo": {
    "name": "t4",
    "slug": "t4",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/t4-icon.png",
    "splash": {
      "image": "./assets/splash-t4.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/t4-adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "softwareKeyboardLayoutMode":"pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
